{
  "name": "related-uuid",
  "version": "1.0.0",
  "description": "Create a consistent UUID based off another UUID and a key",
  "main": "index.js",
  "scripts": {
    "test": "npm run cover",
    "cover": "istanbul cover --report cobertura --print detail node_modules/.bin/cached-tape -- test/index.js && istanbul check-coverage --branches=100",
    "view-cover": "istanbul report html && opn ./coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/robskillington/related-uuid.git"
  },
  "keywords": [
    "utility",
    "uuid"
  ],
  "author": "Rob Skillington",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robskillington/related-uuid/issues"
  },
  "homepage": "https://github.com/robskillington/related-uuid",
  "devDependencies": {
    "cached-tape": "^2.0.0",
    "istanbul": "^0.3.5",
    "lil-uuid": "^0.1.0"
  },
  "dependencies": {
    "node-uuid": "^1.4.2"
  }
}
